{
  "name": "@myadbox/algolia-mock-server",
  "version": "1.0.0",
  "description": "A mock server mimicking Algolia's search index for e2e testing purposes",
  "author": "MyAdbox R&D",
  "license": "MIT",
  "bin": {
    "algolia-mock-server": "./bin/cli.js"
  },
  "main": "bin/cli.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev ./src/cli.ts",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.10",
    "@types/search-index": "^2.1.1",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.5.0",
    "ts-node-dev": "^1.1.8",
    "tslint": "^6.1.3",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "level-party": "^5.1.1",
    "qs": "^6.10.1",
    "search-index": "^2.4.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/myadbox/algoliamockserver"
  },
  "bugs": {
    "url": "https://github.com/myadbox/algoliamockserver/issues"
  },
  "keywords": [
    "alolia",
    "mock server",
    "e2e",
    "nodejs",
    "express",
    "typescript",
    "leveldown",
    "level-party"
  ]
}
